{
  "version": "0.0.2",
  "keywords": ["strapi", "custom-fields"],
  "type": "commonjs",
  "exports": {
    "./package.json": "./package.json",
    "./strapi-admin": {
      "types": "./dist/admin/src/index.d.ts",
      "source": "./admin/src/index.ts",
      "import": "./dist/admin/index.mjs",
      "require": "./dist/admin/index.js",
      "default": "./dist/admin/index.js"
    },
    "./strapi-server": {
      "types": "./dist/server/src/index.d.ts",
      "source": "./server/src/index.ts",
      "import": "./dist/server/index.mjs",
      "require": "./dist/server/index.js",
      "default": "./dist/server/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "strapi-plugin build",
    "watch": "strapi-plugin watch",
    "watch:link": "strapi-plugin watch:link",
    "verify": "strapi-plugin verify",
    "test:ts:front": "run -T tsc -p admin/tsconfig.json",
    "test:ts:back": "run -T tsc -p server/tsconfig.json"
  },
  "dependencies": {
    "@strapi/design-system": "^2.0.0-rc.23",
    "@strapi/icons": "^2.0.0-rc.23",
    "react-intl": "^7.1.11",
    "lucide-react": "0.511.0"
  },
  "devDependencies": {
    "@strapi/strapi": "^5.13.0",
    "@strapi/sdk-plugin": "^5.3.2",
    "prettier": "^3.5.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.30.0",
    "styled-components": "^6.1.18",
    "@types/react": "^19.1.4",
    "@types/react-dom": "^19.1.5",
    "@strapi/typescript-utils": "^5.13.0",
    "typescript": "^5.8.3"
  },
  "peerDependencies": {
    "@strapi/strapi": "^5.13.0",
    "@strapi/sdk-plugin": "^5.3.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.30.0",
    "styled-components": "^6.1.18"
  },
  "strapi": {
    "kind": "plugin",
    "name": "month-multi-selector",
    "displayName": "Month Multi Selector",
    "description": "A simple multi month selector"
  },
  "name": "month-multi-selector",
  "description": "A simple multi month selector",
  "license": "MIT",
  "author": "Mick Schouten <bravebox@gmail.com>"
}
